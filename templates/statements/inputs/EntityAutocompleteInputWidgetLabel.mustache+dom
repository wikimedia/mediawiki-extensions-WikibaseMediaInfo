<span class="wbmi-autocomplete-option" data-mw-url={{url}}>
	<span class="wbmi-autocomplete-option__label">
		{{label}}
	</span>

	{{#aliases}}
	<span class="wbmi-autocomplete-option__aliases">
		{{aliases}}
	</span>
	{{/aliases}}

	{{#description}}
	<span class="wbmi-autocomplete-option__description">
		{{description}}
	</span>
	{{/description}}
</span>
